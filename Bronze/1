//BaekJoon_10808
//알파벳 개수

/*
* 제한 시간 : 0ms / 1s
* 메모리 제한 : 2024KB / 128MB
* 정답 비율 : 69.839%
*/

#include <iostream>
#include <vector>

int main() {

	std::string S = {};
	std::vector<int> ans(26);

	std::cin >> S;

	for (int i = 0; i < size(S); i++)
	{
		ans[S[i]-97]++;
	}


	for (int i = 0; i < 26; i++)
	{
		std::cout << ans[i] << " ";
	}
	

	return 0;
}
